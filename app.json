{
  "expo": {
    "name": "TryBlitz",
    "slug": "Prysm",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/BlitzIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/BlitzIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.blitz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#101010"
      },
      "package": "com.anonymous.blitz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f9e5c88-c79c-4b01-ade7-a6fea394c478"
      }
    }
  }
}
